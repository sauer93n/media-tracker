{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:5173",
      "https://localhost:3000",
      "https://localhost:5173"
    ],
    "AllowCredentials": true,
    "AllowedMethods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "PATCH",
      "OPTIONS"
    ],
    "AllowedHeaders": [
      "*"
    ],
    "ExposedHeaders": [
      "Content-Disposition",
      "X-Total-Count"
    ]
  },
  "CookieOptions": {
    "AccessTokenCookieName": "AccessToken",
    "RefreshTokenCookieName": "RefreshToken",
    "Domain": null,
    "Path": "/",
    "HttpOnly": true,
    "Secure": false,
    "SameSite": "Lax"
  },
  "ApplicationOptions": {
    "KafkaBroker": {
      "BootstrapServers": "localhost:9092",
      "Topic": "media-tracker-domain-events"
    }
  }
}
